{
    "name": "jauntin/pdf-platform-sdk",
    "description": "SDK for integrating with Jauntin's PDF platform",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "jauntin",
        "pdf-platform-sdk"
    ],
    "authors": [
        {
            "name": "Sid Momin",
            "email": "sid@jauntin.com",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/jauntin/pdf-platform-sdk",
    "require": {
        "php": ">=8.3 <8.4",
        "illuminate/support": "^10.0 || ^11.0 || ^12.0",
        "jawira/case-converter": "^3.4"
    },
    "require-dev": {
        "laravel/pint": "^1.21.2",
        "orchestra/testbench": "^8.27.2 || ^9.5.2 || ^10.1.0",
        "phpstan/phpstan": "^2.1.8",
        "phpunit/phpunit": "^10.5.38 || ^11.5.13"
    },
    "autoload": {
        "psr-4": {
            "Jauntin\\PdfPlatformSdk\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jauntin\\PdfPlatformSdk\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jauntin\\PdfPlatformSdk\\PdfPlatformSdkServiceProvider"
            ]
        }
    }
}
