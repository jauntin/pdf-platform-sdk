{
    "name": "jauntin/pdf-platform-sdk",
    "description": "SDK for integrating with Jauntin's PDF platform",
    "keywords": [
        "jauntin",
        "pdf-platform-sdk"
    ],
    "homepage": "https://github.com/jauntin/pdf-platform-sdk",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Sid Momin",
            "email": "sid@jauntin.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0",
        "jawira/case-converter": "^3.4"
    },
    "require-dev": {
        "nunomaduro/larastan": "^1.0",
        "orchestra/testbench": "^7.0",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "Jauntin\\PdfPlatformSdk\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jauntin\\PdfPlatformSdk\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "analyze": "vendor/bin/phpcs && vendor/bin/phpstan analyse --memory-limit=1G",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jauntin\\PdfPlatformSdk\\PdfPlatformSdkServiceProvider"
            ]
        }
    }
}
